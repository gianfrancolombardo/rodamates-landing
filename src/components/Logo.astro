---
import appIcon from '../assets/app_icon_xs.png';

interface Props {
  class?: string;
  color?: string;
}

const { class: className = "", color } = Astro.props;
---

<img 
  src={appIcon.src}
  alt="ReNomada Logo"
  class={`${className || 'w-8 h-8'}`}
  width={appIcon.width}
  height={appIcon.height}
/>
